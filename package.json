{
  "name": "@constructorfleet/ultimate-govee",
  "displayName": "Ulitmate Govee",
  "version": "0.22.6",
  "private": false,
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Library for interacting with Govee devices written in Typescript.",
  "license": "GPL-3.0-or-later",
  "author": "@Teagan42",
  "engines": {
    "node": ">=20.7.0"
  },
  "keywords": [
    "govee",
    "iot",
    "lights",
    "devices"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc && npx tsc-alias",
    "prepublishOnly": "npm run build && npm run lint && npm run format && npm run test",
    "format": "prettier --write \"test/**/*.ts\" \"lib/**/*.ts\"",
    "lint": "eslint \"{lib,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "nest build && jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/microservices": "^10.3.3",
    "@nestjs/platform-express": "^10.3.3",
    "@santi100/equal-lib": "^1.0.9",
    "@types/node-ipc": "^9.2.3",
    "@types/supertest": "^6.0.2",
    "aws-iot-device-sdk-v2": "^1.19.1",
    "axios": "^1.6.7",
    "base64-arraybuffer": "^1.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "json-stringify-safe": "^5.0.1",
    "jwt-decode": "^4.0.0",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "mqtt": "^5.3.6",
    "nest-commander": "^3.12.5",
    "node-ipc": "^10.1.0",
    "node-rsa": "^1.1.1",
    "pem": "^1.14.8",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "ts-md5": "^1.3.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.11.20",
    "@types/node-rsa": "^1.1.4",
    "@types/pem": "^1.14.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-mocha": "^10.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.7.0",
    "jest-module-name-mapper": "^0.1.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "@abandonware/bluetooth-hci-socket": "0.5.3-11",
    "@abandonware/noble": "^1.9.2-24"
  },
  "_moduleAliases": {
    "~ultimate-govee-common": "dist/common",
    "~ultimate-govee-data": "dist/data",
    "~ultimate-govee-domain": "dist/domain"
  }
}
